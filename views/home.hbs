            <div class="form-group">
              <input class="form-control" type="text" id="exampleFormControlLaw" placeholder="Username" value="">
            </div>
            <div class="form-group">
              <input class="form-control" type="password" id="exampleFormControlLaww" placeholder="Password" value="">
            </div>
            <button type="button" onClick="redirect()" class="btn btn-block btn-light btn-sm">Log In</button>
          </div>

        </div>
