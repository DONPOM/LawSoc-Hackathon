<div class="box">
  <div class="box-header">
    <h3 class="box-title">Progress</h3>
  </div>
  <!-- /.box-header -->
  <div class="box-body ">

    <h3>Settlement</h3>
    <table class="progress-table">
      <tr>
        <td class="progress-cell ">
          Offer Made
        </td>
        <td class="progress-cell ">
          Counter Offer Made
        </td>
        <td class="progress-cell ">
          Outcome: Rejected
        </td>
      </tr>
    </table>
    <div class="progress">
      <div class="progress-bar progress-bar-red sm" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
        <span class="sr-only">100% Complete (success)</span>
      </div>
    </div>

    <h3>Costs Assessments Application</h3>
    <table class="progress-table">
      <tr>
        <td class="progress-cell ">
          Application Preperation
        </td>
        <td class="progress-cell ">
          Application Submission
        </td>
        <td class="progress-cell ">
          Application Received
        </td>
        <td class="progress-cell ">
          Application Approval
        </td>
        <td class="progress-cell ">
          Cost Estimate
        </td>
      </tr>
    </table>
    <div class="progress progress-sm active">
      <div class="progress-bar progress-bar-success progress-bar-striped" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 20%">
        <span class="sr-only">20% Complete</span>
      </div>
    </div>

    <h3>Costs Assessment</h3>
    <table class="progress-table">
      <tr>
        <td class="progress-cell ">
          Assessor Assignment
        </td>
        <td class="progress-cell ">
          Bill Assessment
        </td>
        <td class="progress-cell ">
          Outcome:
        </td>
      </tr>
    </table>
    <div class="progress progress-sm active">
      <div class="progress-bar progress-bar-success progress-bar-striped" role="progressbar" aria-valuenow="5" aria-valuemin="0" aria-valuemax="100" style="width: 1%">
        <span class="sr-only">0% Complete</span>
      </div>
    </div>
</div>
</div>


